<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.kh.replay.global.auth.model.dao.LocalMapper">
    <insert id="signUp" parameterType="com.kh.replay.global.auth.local.model.dto.LocalDTO">
        INSERT INTO TB_LOCAL 
        (MEMBER_ID, 
        PASSWORD,
        )
        VALUES(
        #{memberId},
        #{password}
        )
    </insert>

</mapper>